{
  "name": "@identity-connect/crypto",
  "version": "0.2.4",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "dependencies": {
    "@aptos-connect/wallet-api": "^0.1.0",
    "@noble/hashes": "^1.3.1",
    "ed2curve": "^0.3.0",
    "tweetnacl": "^1.0.3"
  },
  "peerDependencies": {
    "@aptos-labs/ts-sdk": "1.18.1"
  },
  "devDependencies": {
    "@types/ed2curve": "^0.2.2",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.11",
    "jest": "^29.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsup": "^7.1.0",
    "typescript": "5.0.4",
    "@identity-connect/eslint-config": "0.0.0",
    "@identity-connect/tsconfig": "0.0.0"
  },
  "files": [
    "dist",
    "src",
    "!src/**.test.ts",
    "!src/**/__tests__"
  ],
  "scripts": {
    "build:bundle": "tsup src/index.ts --tsconfig tsconfig.build.json --format cjs,esm --sourcemap",
    "build:declarations": "tsc --project tsconfig.build.json --emitDeclarationOnly --declaration --declarationMap",
    "build": "pnpm build:bundle && pnpm build:declarations",
    "lint": "prettier --check src",
    "lint:fix": "prettier --write src && eslint --cache ./src --fix",
    "test": "jest"
  }
}