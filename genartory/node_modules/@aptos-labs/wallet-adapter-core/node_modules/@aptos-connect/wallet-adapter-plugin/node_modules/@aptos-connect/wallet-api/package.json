{
  "name": "@aptos-connect/wallet-api",
  "version": "0.1.1",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "dependencies": {
    "@identity-connect/api": "^0.6.1"
  },
  "peerDependencies": {
    "@aptos-labs/wallet-standard": "^0.1.0",
    "@aptos-labs/ts-sdk": "1.18.1",
    "aptos": "^1.20.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "jest": "^29.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsup": "^7.1.0",
    "typescript": "5.0.4",
    "@identity-connect/tsconfig": "0.0.0",
    "@identity-connect/eslint-config": "0.0.0"
  },
  "files": [
    "dist",
    "src",
    "!src/**.test.ts",
    "!src/**/__tests__"
  ],
  "scripts": {
    "build:bundle": "tsup src/index.ts --tsconfig tsconfig.build.json --format cjs,esm --sourcemap",
    "build:declarations": "tsc --project tsconfig.build.json --emitDeclarationOnly --declaration --declarationMap",
    "build": "pnpm build:bundle && pnpm build:declarations",
    "lint": "prettier --check src",
    "lint:fix": "prettier --write src && eslint --cache ./src --fix"
  }
}